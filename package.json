  "bugs": {
    "url": "https://github.com/zlot/dev-quickstart/issues"
  },
  "homepage": "https://github.com/zlot/dev-quickstart#readme",
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "babelify": "^7.3.0",
    "budo": "^9.4.1",
    "faucet": "0.0.1",
    "tape": "^4.6.3"
  },
  "dependencies": {
    "lodash.sample": "^4.2.1",
    "superagent": "^3.3.1"
  }
}
